# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

GET     /testgenerate/test/:scaleCode/:code            controllers.ArchivesManageController.test(scaleCode:String, code:Integer)

GET     /testgenerate/test/:scaleCode2            controllers.ArchivesManageController.test2(scaleCode2:Integer)

GET     /department/saveDepartment/:name                       controllers.ArchivesManageController.saveDepartment(name:String)

GET     /department/deleteDepartment/:deptId                   controllers.ArchivesManageController.deleteDepartment(deptId:Long)

GET     /department/getHeritanceRootDepartment                 controllers.ArchivesManageController.getHeritanceRootDepartment

GET     /department/addDepartment/:name/:parentId              controllers.ArchivesManageController.addDepartment(name:String,parentId:Long)

GET     /department/createDepartment/:name                     controllers.ArchivesManageController.createDepartment(name:String)

GET     /department/moveDepartment/:movedId/:parentId           controllers.ArchivesManageController.moveDepartment(movedId:Long,parentId:Long)